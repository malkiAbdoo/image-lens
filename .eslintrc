{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "no-console": "warn",
    "@typescript-eslint/no-var-requires": "off",
    "no-unused-vars": "warn",
    "no-undef": "off"
  }
}
